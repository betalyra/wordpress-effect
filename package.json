  },
  "devDependencies": {
    "@effect/vitest": "^0.23.10",
    "@total-typescript/tsconfig": "^1.0.4",
    "@vitest/coverage-v8": "3.2.4",
    "pkg-pr-new": "^0.0.54",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "vitest": "^3.2.4",
    "zod": "^3.25.67"
